<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:up="urn:import:gwtupload.client">
	<g:HTMLPanel>
		<g:Label text="First Name" ui:field="firstName" />
		<g:Label text="Last Name" ui:field="lastName" />
		<g:FlowPanel>
			<g:Image ui:field="profilePic" />
			<up:MultiUploader type="BUTTON" ui:field="uploader"
				maximumFiles="2" avoidRepeatFiles="true" validExtensions="jpg, png" />
		</g:FlowPanel>
	</g:HTMLPanel>
</ui:UiBinder>

