<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel width="318px" height="29px">
			<g:Label text="Sign in to your account"/>
			<g:LayoutPanel width="394px" height="150px">
			<g:layer left="10px" width="69px" top="26px" height="18px">
				<g:Label text="Username:"/>
			</g:layer>
			<g:layer left="10px" width="60px" top="60px" height="18px">
				<g:Label text="Password:"/>
			</g:layer>
			<g:layer left="75px" width="200px" top="19px" height="25px">
				<g:TextBox ui:field="textBoxUsername" width="150px"/>
			</g:layer>
			<g:layer left="76px" width="200px" top="53px" height="25px">
				<g:PasswordTextBox ui:field="textBoxPassword" width="150px"/>
			</g:layer>
			<g:layer left="85px" width="288px" top="88px" height="20px">
				<g:CheckBox text="Remember me on this computer"/>
			</g:layer>
			<g:layer left="81px" top="114px" height="24px" right="234px">
				<g:Button text="Sign In" ui:field="button"/>
			</g:layer>
		</g:LayoutPanel>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 