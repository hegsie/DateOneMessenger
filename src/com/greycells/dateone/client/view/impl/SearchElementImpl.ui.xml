<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:LayoutPanel width="229px" height="245px">
			<g:layer left="0px" width="100px" top="0px" height="100px">
				<g:Image/>
			</g:layer>
			<g:layer left="0px" width="153px" top="227px" height="18px">
				<g:Label text="Name of Dating User" ui:field="_displayName"/>
			</g:layer>
			<g:layer left="144px" width="85px" top="221px" height="24px">
				<g:Button text="View" ui:field="_viewProfile"/>
			</g:layer>
			<g:layer left="0px" width="229px" top="0px" height="215px">
				<g:FlowPanel ui:field="DisplayPanel"/>
			</g:layer>
		</g:LayoutPanel>
	</g:HTMLPanel>
</ui:UiBinder> 